{
  "name": "js-starter",
  "version": "0.0.1",
  "description": "A boilerplate for starting js code",
  "keywords": [
    "js-starter",
    "boilerplate"
  ],
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint \"src/**/*.js\"",
    "start": "parcel ./src/components/index.html",
    "build": "parcel build ./src/components/index.html",
    "now-build": "parcel build ./src/components/index.html --out-dir public"
  },
  "author": "Build For SDG <buildforsdg@andela.com>",
  "homepage": "https://github.com/BuildForSDG/js-starter#readme",
  "repository": "https://github.com/BuildForSDG/js-starter.git",
  "bugs": {
    "url": "https://github.com/BuildForSDG/js-starter/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.0",
    "eslint-plugin-react": "^7.20.0",
    "jquery": "^3.5.1",
    "mdbreact": "^4.27.0",
    "prop-type": "^0.0.1",
    "react": "^16.2.0",
    "react-alert": "^7.0.1",
    "react-alert-template-basic": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-flutterwave-rave": "^1.0.4",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "reactstrap": "^8.4.1",
    "redux": "^3.1.0 || ^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.1.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.10.1",
    "babel-jest": "^25.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.4.0",
    "jest-enzyme": "^7.1.2",
    "parcel-bundler": "^1.12.4"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
